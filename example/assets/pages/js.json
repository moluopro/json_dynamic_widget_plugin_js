{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "JS"
          }
        }
      }
    },
    "body": {
      "type": "list_view",
      "children": [
        {
          "type": "padding",
          "args": {
            "padding": [
              16.0,
              8.0
            ]
          },
          "child": {
            "type": "text",
            "args": {
              "text": "${'Today is ' + js('new Date().toString()')}"
            }
          }
        }
      ]
    }
  }
}